{
  "name": "react-native-zyre",
  "version": "0.1.1",
  "description": "React-Native port of Zyre - an open-source framework for proximity-based peer-to-peer applications",
  "main": "index.js",
  "scripts": {
    "make-patch": "patch-package zyre.js",
    "build:docs": "jsdoc -R README.md -d docs lib/*.js",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olehs/react-native-zyre.git"
  },
  "keywords": [
    "react-native",
    "zyre",
    "zre",
    "zeromq",
    "zmq",
    "libzmq",
    "p2p",
    "peer",
    "peer-to-peer"
  ],
  "author": "Oleg Sokolov",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/olehs/react-native-zyre/issues"
  },
  "homepage": "https://github.com/olehs/react-native-zyre#readme",
  "dependencies": {
    "buffer": "^5.6.0",
    "debug": "^4.1.1",
    "patch-package": "^6.2.2",
    "zyre.js": "olehs/zyre.js#beta"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "jsdoc": "^3.6.3"
  },
  "peerDependencies": {
    "@olehs/react-native-zeromq": ">= 0.8",
    "react-native-get-random-values": ">= 1.4",
    "react-native-os": ">= 1.2",
    "react-native-tcp-socket": ">= 3.5",
    "react-native-udp": ">= 3.1"
  },
  "greenkeeper": {
    "ignore": [
      "eslint",
      "eslint-config-airbnb-base",
      "eslint-plugin-import"
    ]
  }
}
